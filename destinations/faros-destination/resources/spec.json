{
  "type": "SPEC",
  "spec": {
  "documentationUrl": "https://docs.faros.ai",
    "connectionSpecification": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Faros Spec",
      "type": "object",
      "required": [
        "api_url",
        "api_key"
      ],
      "additionalProperties": false,
      "properties": {
        "api_url": {
          "type": "string",
          "title": "API URL",
          "description": "The Faros API URL.",
          "examples": ["https://prod.api.faros.ai"]
        },
        "api_key": {
          "title": "API Key",
          "type": "string",
          "description": "The Faros API key to use to hit the API.",
          "airbyte_secret": true
        }
      }
    }
  }
}